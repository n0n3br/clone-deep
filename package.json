{
  "name": "clone-deep",
  "version": "1.0.0",
  "description": "Nossa biblioteca do curso de JS com TDD na prática.",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n0n3br/clone-deep"
  },
  "keywords": [
    "js",
    "object",
    "library",
    "clone",
    "deep"
  ],
  "author": "Rogério Luiz Aques de Amorim <rogeriol@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/n0n3br/clone-deep/issues"
  },
  "homepage": "https://github.com/n0n3br/clone-deep#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^0.11.9",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.6"
  },
  "dependencies": {
    "braces": "^2.3.2",
    "growl": "^1.10.5",
    "js-yaml": "^3.13.0",
    "lodash": "^4.17.11"
  }
}
